{
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "all",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^@/((?!css).+)",
    "^[./]",
    "^(.+).css"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
